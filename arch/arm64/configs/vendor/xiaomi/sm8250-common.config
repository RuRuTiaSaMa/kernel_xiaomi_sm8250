# Kernel
CONFIG_LOCALVERSION="-Maou"
CONFIG_LOCALVERSION_AUTO=y

# DTBO
CONFIG_BUILD_ARM64_DT_OVERLAY=y
CONFIG_BUILD_ARM64_APPENDED_DTB_IMAGE=y

# QCACLD
CONFIG_QCA_CLD_WLAN=y
CONFIG_QCA_CLD_WLAN_PROFILE="qca6390"

# DebugFS
CONFIG_DEBUG_FS=n
CONFIG_MSM_TZ_LOG=n
CONFIG_CMA_DEBUGFS=n
CONFIG_REGMAP_ALLOW_WRITE_DEBUGFS=n
CONFIG_IPA_UT=n
CONFIG_IOMMU_TESTS=n
CONFIG_IOMMU_TESTS=n
CONFIG_QMP_DEBUGFS_CLIENT=n

# exFAT
CONFIG_EXFAT_FS=y

# Persistent store
CONFIG_PSTORE=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y
CONFIG_CMDLINE="cgroup_disable=pressure ramoops_memreserve=4M"
CONFIG_BLK_WBT=y
CONFIG_BLK_WBT_SQ=y
CONFIG_MTD=y
CONFIG_MTD_BLOCK=y
CONFIG_MTD_OOPS=y
CONFIG_MTD_BLOCK2MTD=y

# Backlight
CONFIG_BACKLIGHT_KTZ8866=y
CONFIG_BACKLIGHT_DUALKTZ8866=y

# Camera motor
CONFIG_MOTOR_DRV8846=y

# GPS
CONFIG_AIROHA_GPS=y

# Hall sensor
CONFIG_HALL_AKM09970=y

# ION
CONFIG_ION_CAMERA_HEAP=y

# IR
CONFIG_RC_CORE=y
CONFIG_LIRC=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y
CONFIG_IR_SPI_MI=y

# Keyboard
CONFIG_INPUT_XIAOMI_KEYBOARD=y
CONFIG_KEYBOARD_NANO_8030=y

# LED
CONFIG_LEDS_AW2015=y

# Power
CONFIG_FRAME_WARN=4096
CONFIG_SMB1355_SLAVE_CHARGER=n
CONFIG_SMB1390_CHARGE_PUMP_PSY=n
CONFIG_SMB1398_CHARGER=n

# Regulator
CONFIG_I2C_RETRY=y
CONFIG_REGULATOR_PM8008=y
CONFIG_REGULATOR_QCOM_RPMH=y

# Touchscreen
CONFIG_TOUCHSCREEN_FTS=n
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX=n
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM=n
CONFIG_TOUCHSCREEN_ST=n
CONFIG_TOUCHSCREEN_COMMON=y

# USB
CONFIG_PS5169=y

# Vibrator
CONFIG_INPUT_QTI_HAPTICS=n

# Cpu Frequencies Statistics
CONFIG_CPU_FREQ_STAT=y

# Net
CONFIG_TCP_CONG_ADVANCED=y
CONFIG_TCP_CONG_BBR=y
CONFIG_TCP_CONG_BBR2=y
CONFIG_DEFAULT_BBR2=y
CONFIG_DEFAULT_TCP_CONG="bbr2"
CONFIG_NETFILTER_XT_TARGET_HL=y
CONFIG_NETFILTER_XT_TARGET_HMARK=y
CONFIG_IP_NF_TARGET_TTL=y

# ZSTD
CONFIG_CRYPTO_ZSTD=y
CONFIG_ZSTD_COMMON=y
CONFIG_ZRAM_DEF_COMP_ZSTD=y

# LZ4
CONFIG_CRYPTO_LZ4=y
CONFIG_KERNEL_LZ4=y

# Enable LZ4 Ramdisk
CONFIG_RD_LZ4=y

# Inline parts that usually build as =m (Modules)
CONFIG_WIGIG_SENSING_SPI=y
CONFIG_WIL6210=y
CONFIG_MSM_RDBG=y
CONFIG_DVB_MPQ=y
CONFIG_DVB_MPQ_DEMUX=y
CONFIG_MSM_11AD=y
CONFIG_QCOM_LLCC_PERFMON=y

# QTI Smart Link Aggregation driver
CONFIG_SLA_STAIC_COLLECT=y

# Sched
# CONFIG_SCHED_TUNE is not set
# CONFIG_CPU_BOOST is not set
# CONFIG_SCHED_CORE_CTL is not set
CONFIG_UCLAMP_TASK=y
CONFIG_UCLAMP_TASK_GROUP=y
CONFIG_UCLAMP_BUCKETS_COUNT=20
CONFIG_WQ_POWER_EFFICIENT_DEFAULT=y

#Usb fast charge
CONFIG_FORCE_FAST_CHARGE=y

# Zram
CONFIG_ZRAM_WRITEBACK=y
CONFIG_ZRAM_MEMORY_TRACKING=y
CONFIG_ZRAM_MULTI_COMP=y

CONFIG_THIN_ARCHIVES=y
CONFIG_LTO=y
CONFIG_LTO_CLANG=y
CONFIG_THINLTO=y
CONFIG_ARCH_HAS_RELR=y
CONFIG_RELR=y